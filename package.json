{
  "name": "theautodoctor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next dev --turbo",
    "dev:fast": "cross-env NODE_OPTIONS='--max-old-space-size=4096' NODE_ENV=development next dev --turbo",
    "dev:webpack": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "dev:clean": "rm -rf .next && pnpm dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start -H 0.0.0.0 -p ${PORT:-3000}",
    "lint": "next lint",
    "lint:strict": "eslint . --max-warnings 10",
    "typecheck": "tsc --noEmit",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:unit": "playwright test tests/unit",
    "test:e2e": "playwright test tests/e2e",
    "test:integration": "playwright test tests/e2e/database-integrity.spec.ts",
    "test:rls": "playwright test tests/e2e/rls-policies.spec.ts",
    "audit:security": "pnpm audit --audit-level=high",
    "audit:deps": "depcheck --ignores=\"@types/*,eslint-config-next\"",
    "audit:all": "pnpm run typecheck && pnpm run lint:strict && pnpm run audit:deps && pnpm run audit:security",
    "validate": "pnpm run typecheck && pnpm run lint && pnpm run build",
    "clean": "rm -rf .next node_modules/.cache",
    "config:dev": "cp next.config.dev.js next.config.js",
    "config:prod": "cp next.config.prod.js next.config.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@livekit/components-react": "^2.9.15",
    "@livekit/components-styles": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@react-pdf/renderer": "^4.3.1",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.75.1",
    "@types/react-signature-canvas": "^1.0.7",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.6",
    "autoprefixer": "^10.4.16",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "file-type": "^21.0.0",
    "framer-motion": "^11.18.2",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "livekit-client": "^2.15.11",
    "livekit-server-sdk": "^2.14.0",
    "lucide-react": "^0.460.0",
    "next": "^14.2.11",
    "postcss": "^8.4.32",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hot-toast": "^2.6.0",
    "react-select": "^5.10.2",
    "react-signature-canvas": "^1.1.0-alpha.2",
    "recharts": "^3.3.0",
    "resend": "^3.2.0",
    "stripe": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@types/node": "^20.19.23",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.7",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.2.33",
    "supabase": "^2.53.6",
    "typescript": "^5.3.3",
    "zod": "^4.1.12"
  },
  "next": {
    "instrumentationHook": "src/instrumentation.ts"
  }
}
